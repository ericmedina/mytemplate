{
    "name": "pampaui",
    "version": "0.1.4",
    "description": "Paquete simple de elementos para dashboards",
    "main": ".dist/pampaui.common.js",
    "private": false,
    "author": {
        "name": "Eric Medina",
        "email": "ericmedina.dev@gmail.com"
    },
    "repository": "https://github.com/ericmedina/pampaui",
    "bugs": "https://github.com/ericmedina/pampaui/issues",
    "keywords": [
        "Datatable",
